pipeline{
    agent any

    tools{
        maven 'mvn'
    }

    stages{


        stage('checkout'){
            steps{
                checkout scm 
            }
        }

        stage('Test'){

            steps{
                sh'''

                mvn test

                '''
            }
        }

        stage('Archive'){

            steps{
                archiveArtifacts artifacts: 'target/*.jar' , fingerprint: true

            }
        }
    }
}